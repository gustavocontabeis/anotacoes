   <text:p text:style-name="code">code <text:span text:style-name="T39">~</text:span>/.profile </text:p>
   <text:p text:style-name="P117"><text:tab/><text:span text:style-name="T35">cole o conte</text:span><text:span text:style-name="T39">ú</text:span><text:span text:style-name="T35">do</text:span></text:p>
   <text:p text:style-name="code"/>
   <text:p text:style-name="P117"><text:span text:style-name="T35"># execute o com</text:span><text:span text:style-name="T39">a</text:span><text:span text:style-name="T35">nto abaixo para recarregar os arquivos e poder testar a instalação</text:span></text:p>
   <text:p text:style-name="code">cd ~</text:p>
