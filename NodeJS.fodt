   <text:p text:style-name="P47"><text:tab/>“<text:span text:style-name="T27">tests</text:span>”: “echo \&quot;Error: no test specified\&quot; &amp;&amp; exit 1”</text:p>
   <text:p text:style-name="P47"/>
   <text:p text:style-name="P48">}</text:p>
   <text:p text:style-name="P45"/>
   <text:p text:style-name="P31"/>
   <text:p text:style-name="P33">Se e<text:span text:style-name="T33">x</text:span>ecutarmos</text:p>
   <text:p text:style-name="code"><text:span text:style-name="T33">npm</text:span> test</text:p>
   <text:p text:style-name="P33">Será mostrado: </text:p>
   <text:p text:style-name="P33">Ser adicionarmos</text:p>
   <text:p text:style-name="P47"><text:tab/>&quot;start&quot;: &quot;nodemon server.js&quot;</text:p>
