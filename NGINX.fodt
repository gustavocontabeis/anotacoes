   <text:p text:style-name="P21"/>
   <text:p text:style-name="P21"><text:tab/>server_name localhost;</text:p>
   <text:p text:style-name="P21"/>
   <text:p text:style-name="P21"><text:tab/>location / {</text:p>
   <text:p text:style-name="P21"><text:tab/><text:tab/>proxy_pass http://servicos;</text:p>
   <text:p text:style-name="P21"><text:tab/>}</text:p>
