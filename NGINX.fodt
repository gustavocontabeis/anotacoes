   <text:p text:style-name="P16">Configuração do load balancer</text:p>
   <text:p text:style-name="P21"/>
   <text:p text:style-name="P21">upstream servicos {</text:p>
   <text:p text:style-name="P21"><text:tab/>server localhost:8001;</text:p>
