   <text:h text:style-name="Heading_20_2" text:outline-level="2"><text:bookmark-start text:name="__RefHeading___Toc290_2511433097"/>Permisses<text:bookmark-end text:name="__RefHeading___Toc290_2511433097"/></text:h>
   <text:p text:style-name="P37">Formato binrio</text:p>
   <text:p text:style-name="Text_20_body">Read<text:span text:style-name="T25">=4</text:span>,write<text:span text:style-name="T25">=2</text:span>,execute<text:span text:style-name="T25">=1</text:span> <text:span text:style-name="T25">(rwx)</text:span></text:p>
