     <text:p text:style-name="P52"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc1293_710169630" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Spring Batch<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P52"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc168_3567078869" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Spring Security<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P76"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc201_31808727" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Origem do usuário<text:tab/>4</text:a></text:p>
